import java.util.ArrayList;
import java.util.Collections;
import java.util.Random;
import java.util.Scanner;
public class Main {
public static void main(String[] args) {
Random ran = new Random();
int sixDigit = ran.nextInt(900000)+100000;
int fourDigit = ran.nextInt(9000)+1000;
Scanner scan = new Scanner(System.in);
System.out.println("Enter 4 digit number");
int four = scan.nextInt();
System.out.println("Enter 6 digit number");
int six = scan.nextInt();
ArrayList<Integer> names = new
ArrayList<Integer>();
names.add(sixDigit);
names.add(fourDigit);
Collections.shuffle(names);
int n = names.get(0);
if(n == four || n == six) {
System.out.println("Correct OTP");
 }
 else {
 System.out.println("Wrong OTP");
 }
 System.out.println("OTP is " + n);
 }
} 
